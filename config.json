{
  "name": "Hassio Home Display",
  "version": "dev",
  "slug": "home_display",
  "description": "UI for a wall mounted Home Assistant control panel",
  "url": "https://github.com/switchtrue/hassio-home-display",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:7080]",
  "startup": "system",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "boot": "auto",
  "hassio_api": true,
  "map": [
    "ssl"
  ],
  "ports": {
    "7080/tcp": 7080
  },
  "options": {},
  "schema": {},
  "environment": {
    "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
  }
}